Cell[BoxData[
 RowBox[{"Clear", "[", "\"\<Global`*\>\"", "]"}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{"g", "[", 
   RowBox[{"t_", ",", 
    RowBox[{"OptionsPattern", "[", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"A", "\[Rule]", "1"}], ",", 
       RowBox[{"T", "\[Rule]", "1"}], ",", 
       RowBox[{"t1", "\[Rule]", "0"}], ",", 
       RowBox[{"t2", "\[Rule]", 
        FractionBox["1", "2"]}]}], "}"}], "\[IndentingNewLine]", "]"}]}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"A", "=", 
       RowBox[{"OptionValue", "[", "A", "]"}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"T", "=", 
       RowBox[{"OptionValue", "[", "T", "]"}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"t1", "=", 
       RowBox[{"OptionValue", "[", "t1", "]"}]}], ",", "\[IndentingNewLine]", 
      
      RowBox[{"t2", "=", 
       RowBox[{"OptionValue", "[", "t2", "]"}]}], ",", "\[IndentingNewLine]", 
      "tt", ",", "\[IndentingNewLine]", "tt2"}], "\[IndentingNewLine]", "}"}],
     ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"t1", ">", "t2"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"t1", ",", "t2", ",", "A"}], "}"}], "=", 
         RowBox[{"{", 
          RowBox[{"t2", ",", "t1", ",", 
           RowBox[{"-", "A"}]}], "}"}]}], ";"}]}], "\[IndentingNewLine]", 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"tt", ":=", 
      RowBox[{"Mod", "[", 
       RowBox[{
        RowBox[{"t", "-", "t1"}], ",", "T"}], "]"}]}], ";", 
     RowBox[{"(*", " ", 
      RowBox[{"periodic", ",", " ", 
       RowBox[{"translated", " ", "time"}], ",", " ", 
       RowBox[{
        RowBox[{"so", " ", "that", " ", "t"}], "=", 
        RowBox[{
         RowBox[{"t1", " ", "corresponds", " ", "with", " ", "tt"}], "=", 
         "0"}]}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"tt2", " ", ":=", " ", 
      RowBox[{"t2", " ", "-", " ", "t1"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"\[Piecewise]", GridBox[{
        {
         RowBox[{
          RowBox[{"-", "A"}], "+", 
          RowBox[{"tt", " ", 
